<h1>会員一覧画面です</h1>



<table>
  <tr>
    <th>ID</th>
    <th>名前</th>
    <th>メールアドレス</th>
    <th>会員ステータス</th>
  </tr>

  <% @users.each do |user| %>
  <tr>
    <td><%= user.id %></td>
    <td>
      <%= link_to admin_user_path(user.id) do %>
        <%= user.name %>
      <% end %>
    </td>
    <td><%= user.email %></td>
    <td>
      <% if user.is_deleted? %>
        退会
      <% else %>
        有効
      <% end %>
    </td>
  </tr>
  <% end %>

</table>







